{
  "name": "catcafe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "sass": "1.37.5",
    "scss": "0.2.4",
    "styled-components": "5.3.0",
    "swiper": "6.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext .jsx --ext .js src/",
    "svgr": "svgr --ext=jsx -d src/components/icons/ src/assets/icons/svg/",
    "lint:css":"stylelint './src/**/*.(js|jsx)'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "htmlacademy/react"
    ],
    "rules": {
      "react/prop-types": [
        "off"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/cli": "5.5.0",
    "customize-cra": "1.0.0",
    "eslint-config-htmlacademy": "4.0.2",
    "react-app-rewired": "2.1.8",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.10",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
